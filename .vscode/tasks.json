{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Launch App",
        "type": "shell",
        "command": "flutter",
        "args": [
          "run"
        ],
        "group": "build",
        "presentation": {
          "reveal": "always",
          "panel": "new"
        },
        "problemMatcher": []
      },
      {
        "label": "Generate Localization",
        "type": "shell",
        "command": "flutter gen-l10n",
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "echo": true,
          "reveal": "always",
          "focus": false,
          "panel": "shared"
        }
      },
      {
        "label": "Build Runner: Build",
        "type": "shell",
        "command": "flutter pub run build_runner build",
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "echo": true,
          "reveal": "always",
          "focus": false,
          "panel": "shared"
        }
      }
    ]
  }
  